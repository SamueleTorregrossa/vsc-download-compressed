{
  "name": "compress-download-extension",
  "displayName": "SSH Fast Dowload",
  "description": "A simple extension to more quickly download files from a remote server in VSCode.",
  "version": "0.0.5",
  "publisher": "samueletorregrossa",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SamueleTorregrossa/vsc-download-compressed"
  },
  "categories": ["Other"],
  "extensionKind": ["ui", "workspace"],
  "activationEvents": [
    "onCommand:extension.compress_download"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.compress_download",
        "title": "Compress and Download"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.compress_download",
          "when": "remoteName == 'ssh-remote'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.0.3",
    "vscode": "^1.1.37",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.60.0",
    "@types/mocha": "^8.2.2"
  }
}
